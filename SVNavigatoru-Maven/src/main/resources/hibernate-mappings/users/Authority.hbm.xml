<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.svnavigatoru600.domain.users.Authority" table="AUTHORITIES" lazy="false">
		<composite-id name="id" class="com.svnavigatoru600.domain.users.AuthorityId">
			<key-property name="username" type="string" length="50"/>
			<key-property name="authority" type="string" length="50"/>
		</composite-id>
	</class>

</hibernate-mapping>
